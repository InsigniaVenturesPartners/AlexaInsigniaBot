        "import": [
            {
                "name": "alexa-layouts",
                "version": "1.4.0"
            }
        ],
        "mainTemplate": {
            "parameters": [
                "payload"
            ],
            "items": [
                {
                    "type": "Container",
                    "height": "100vh",
                    "width": "100vw",
                    "paddingTop": "@spacingSmall",
                    "bind": [
                        {
                            "name": "sliderThumbPosition",
                            "value": 0
                        },
                        {
                            "name": "videoProgressValue",
                            "type": "number",
                            "value": 0
                        },
                        {
                            "name": "videoTotalValue",
                            "type": "number",
                            "value": 0
                        }
                    ],
                    "items": [
                        {
                            "type": "AlexaBackground"
                        },
                        {
                            "autoplay": true,
                            "audioTrack": "foreground",
                            "onPlay": [
                                {
                                    "type": "SetValue",
                                    "property": "videoTotalValue",
                                    "value": "${event.duration}"
                                }
                            ],
                            "onTrackUpdate": [
                                {
                                    "type": "SetValue",
                                    "property": "videoTotalValue",
                                    "value": "${event.duration}"
                                }
                            ],
                            "onTimeUpdate": [
                                {
                                    "type": "SetValue",
                                    "property": "videoProgressValue",
                                    "value": "${event.currentTime}"
                                },
                                {
                                    "type": "SetValue",
                                    "componentId": "slider",
                                    "property": "progressValue",
                                    "value": "${videoProgressValue}"
                                },
                                {
                                    "type": "SetValue",
                                    "property": "videoTotalValue",
                                    "value": "${event.duration}"
                                }
                            ],
                            "onTrackReady": [
                                {
                                    "type": "SetValue",
                                    "property": "videoTotalValue",
                                    "value": "${event.duration}"
                                }
                            ],
                            "onTrackFail": [
                                {
                                    "type": "SetValue",
                                    "property": "videoTotalValue",
                                    "value": "0"
                                }
                            ],
                            "scale": "best-fit",
